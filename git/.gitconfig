# https://git-scm.com/

[user]
name = Ching Chow
email = ching.chow.sc@gmail.com

[core]
autocrlf = false

[pull]
ff = only

[rebase]
autosquash = true

[alias]
amend = commit --amend
del = branch --delete --force
fixup = commit --fixup
ls = log --graph --pretty='format:%C(auto)%h [%ci]%d %s (%an)'
pick = cherry-pick
